/* pages/aboutMeCode/aboutMeCode.wxss */
.aboutMeCodeMark{
  width: 750rpx;
  height: 100vh;
  background-color: #3c3c3c;
  position: relative;
}
.aboutMeCodeBox{
  width: 600rpx;
  height: 800rpx;
  background-color: #FFFFFF;
  position: absolute;
  left: 50%;
  margin-left: -300rpx;
  top: 50%;
  margin-top: -400rpx;
}
.aboutMeCodeTitle{
  height: 140rpx;
  margin-left: 40rpx;
  margin-top: 30rpx;
}
.aboutMeCodeTitle image{
  width: 140rpx;
  height: 140rpx;
  float: left;
}
.aboutMeCodeTitle text{
  font-size: 48rpx;
  float: left;
  line-height: 140rpx;
  margin-left: 30rpx;
}
.aboutMeCodetwo{
  width: 520rpx;
  height: 520rpx;
  border: 1px solid #3c3c3c;
  margin-left: 40rpx;
  margin-top: 30rpx;
}
.aboutMeCodeText{
  text-align: center;
  display: block;
  font-size: 32rpx;
  margin-top: 20rpx;
}
.canvas {
    width: 520rpx;
    height: 520rpx;
    /* border: 1px solid black; */
}
.canvas image{
  width: 100%;
  height: 100%;
}